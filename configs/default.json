{
  "input": "path/to/input",
  "output": "path/to/output",
  "process":
    [{
      "type": "resize",
      "alg_lq": ["linear", "nearest"],
        "alg_hq": ["linear"],
        "spread": [0.25, 1.1, 0.25],
        "scale": 1,
        "color_fix": false,
         "gamma_correction": false

    }
    ],
  "num_workers": 8,
  "map_type": "thread",


  "tile": {
    "size": 512,
    "no_wb": true
  }
}
